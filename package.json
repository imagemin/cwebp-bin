{
	"name": "cwebp-bin",
	"version": "7.0.1",
	"description": "cwebp wrapper that makes it seamlessly available as a local dependency",
	"license": "MIT",
	"repository": "imagemin/cwebp-bin",
	"type": "module",
	"exports": "./index.js",
	"funding": "https://github.com/imagemin/cwebp-bin?sponsor=1",
	"bin": {
		"cwebp": "cli.js"
	},
	"engines": {
		"node": "^14.13.1 || >=16.0.0"
	},
	"scripts": {
		"postinstall": "node lib/install.js",
		"test": "xo && ava --timeout=180s"
	},
	"files": [
		"cli.js",
		"index.js",
		"lib",
		"vendor/source"
	],
	"keywords": [
		"imagemin",
		"compress",
		"image",
		"img",
		"jpeg",
		"jpg",
		"minify",
		"optimize",
		"png",
		"webp"
	],
	"dependencies": {
		"bin-build": "^3.0.0",
		"bin-wrapper": "^4.0.1"
	},
	"devDependencies": {
		"ava": "^4.1.0",
		"bin-check": "^4.1.0",
		"compare-size": "^3.0.0",
		"execa": "^6.1.0",
		"tempy": "^2.0.0",
		"xo": "^0.48.0"
	}
}
